{
  "extends": "@istanbuljs/nyc-config-typescript",
  "require": ["source-map-support/register"],
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**"
  ],
  "reporter": ["text", "json"],
  "exclude": [
    "**/*.d.ts",
    "build/**/**/*.js",
    "src/index.ts",
    "src/version.ts",
    "karma.conf.js",
    "src/platform/browser/*.ts",
    "test/*.ts",
    "webpack/*.js",
    ".eslintrc.js"
  ],
  "all": true
}
