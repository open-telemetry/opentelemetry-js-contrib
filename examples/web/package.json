{
  "name": "web-examples",
  "private": true,
  "version": "0.26.0",
  "description": "Example of using web plugins in browser",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext=ts,js,mjs",
    "lint:fix": "eslint . --ext=ts,js,mjs --fix",
    "docker:start": "cd ./docker && docker-compose down && docker-compose up",
