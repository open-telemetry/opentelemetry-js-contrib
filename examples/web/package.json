    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-loader": "^8.2.2",
    "ts-loader": "^6.2.2",
    "webpack": "^4.44.2",
