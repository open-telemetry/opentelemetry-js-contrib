{
  "name": "ioredis-example",
  "private": true,
  "version": "0.23.0",
  "description": "Example of HTTP integration with OpenTelemetry",
  "main": "index.js",
  "scripts": {
    "docker:start": "docker run -d -p 6379:6379 --name otjsredis redis:alpine",
    "docker:stop": "docker stop otjsredis && docker rm otjsredis",
    "start": "node index.js"
  },
