{
  "name": "@opentelemetry/baggage-span-processor",
  "version": "0.2.0",
  "description": "OpenTelemetry Baggage Span Processor for Node.js",
  "main": "build/src/index.ts",
  "types": "build/src/index.d.ts",
  "repository": "open-telemetry/opentelemetry-js-contrib",
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "precompile": "tsc --version && lerna run version:update --scope @opentelemetry/baggage-span-processor --include-dependencies",
    "prewatch": "npm run precompile",
    "prepublishOnly": "npm run compile",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "test": "nyc ts-mocha -p tsconfig.json test/**/*.test.ts",
    "version:update": "node ../../scripts/version-update.js",
    "watch": "tsc -w"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "tracing",
    "baggage",
    "plugin"
  ],
  "author": "OpenTelemetry Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0"
  },
  "dependencies": {
    "@opentelemetry/sdk-trace-base": "^1.0.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "@types/mocha": "8.2.3",
    "@types/node": "18.6.5",
    "expect": "29.2.0",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "rimraf": "5.0.5",
    "ts-mocha": "10.0.0",
    "typescript": "4.4.4"
  },
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/packages/baggage-span-processor#readme"
}
