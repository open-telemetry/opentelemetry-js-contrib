{
   "permissions": [
      "activeTab", 
      "scripting",
      "webNavigation",
      "storage"
   ],
   "action": {
      "default_popup": "popup.html"
   },
   "icons": "icons/otel-logo_{size}.png",
   "background":  "background.js",
   "options_ui": {
      "page": "options.html",
      "open_in_tab": true,
   },
   "web_accessible_resources": [
      "instrumentation.js",
      "instrumentation.js.map",
      "contentScript.js.map",
      "js/index.js.map"
   ],
   "host_permissions": [
      '<all_urls>'
   ]
}
