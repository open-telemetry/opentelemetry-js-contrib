{
  "name": "@opentelemetry/instrumentation-neo4j",
  "version": "0.1.0",
  "description": "open telemetry instrumentation for the `neo4j` javascript driver",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib.git",
    "directory": "packages/instrumentation-neo4j"
  },
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "compile:with-dependencies": "nx run-many -t compile -p @opentelemetry/instrumentation-neo4j",
    "lint:readme": "node ../../scripts/lint-readme.js",
    "prepublishOnly": "npm run compile",
    "test": "nyc --no-clean mocha --require '@opentelemetry/contrib-test-utils' 'test/**/*.test.ts'",
    "test:with-services-env": "cross-env NODE_OPTIONS='-r dotenv/config' DOTENV_CONFIG_PATH=../../test/test-services.env npm test",
    "test-all-versions": "tav",
    "test-all-versions:with-services-env": "cross-env NODE_OPTIONS='-r dotenv/config' DOTENV_CONFIG_PATH=../../test/test-services.env npm run test-all-versions",
    "test-services:start": "cd ../.. && npm run test-services:start neo4j",
    "test-services:stop": "cd ../.. && npm run test-services:stop neo4j",
    "version:update": "node ../../scripts/version-update.js",
    "watch": "tsc -w"
  },
  "keywords": [
    "neo4j",
    "neo4j-driver",
    "instrumentation",
    "nodejs",
    "opentelemetry",
    "tracing"
  ],
  "author": "OpenTelemetry Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/contrib-test-utils": "^0.58.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "neo4j-driver": "6.0.1",
    "rxjs": "7.8.2"
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0"
  },
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/packages/instrumentation-neo4j#readme"
}
