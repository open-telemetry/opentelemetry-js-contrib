    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/esm/**/*.js",
    "build/esm/**/*.map",
    "build/esm/**/*.d.ts",
    "build/esnext/**/*.js",
    "build/esnext/**/*.map",
    "build/esnext/**/*.d.ts",
    "build/src/**/*.js",
    "build/src/**/*.map",
    "build/src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
