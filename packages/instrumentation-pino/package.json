{
  "name": "@opentelemetry/instrumentation-pino",
  "version": "0.54.0",
  "description": "OpenTelemetry instrumentation for `pino` logger",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib.git",
    "directory": "packages/instrumentation-pino"
  },
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "compile:with-dependencies": "nx run-many -t compile -p @opentelemetry/instrumentation-pino",
    "lint:readme": "node ../../scripts/lint-readme.js",
    "prepublishOnly": "npm run compile",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "test": "nyc --no-clean mocha 'test/**/*.test.ts'",
    "test-all-versions": "tav",
    "version:update": "node ../../scripts/version-update.js"
  },
  "keywords": [
    "instrumentation",
    "logging",
    "nodejs",
    "opentelemetry",
    "pino",
    "profiling",
    "tracing"
  ],
  "author": "OpenTelemetry Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/contrib-test-utils": "^0.54.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@types/mocha": "10.0.10",
    "@types/node": "18.18.14",
    "@types/semver": "7.5.8",
    "@types/sinon": "17.0.4",
    "nyc": "17.1.0",
    "pino": "^8.21.0",
    "rimraf": "5.0.10",
    "semver": "7.7.3",
    "sinon": "15.2.0",
    "test-all-versions": "6.1.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@opentelemetry/api-logs": "^0.207.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/instrumentation": "^0.207.0"
  },
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/packages/instrumentation-pino#readme"
}
